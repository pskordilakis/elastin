{
  "name": "pskordilakis/elastin",
  "description": "Elasicsearch Query Builder",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Panagiotis Skordilakis",
      "email": "p.skordilakis@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Elastin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "test": "phpunit"
  }
}
