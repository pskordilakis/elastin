{
  "name": "pskordilakis/elastin",
  "description": "Elasticsearch Query Builder",
  "keywords": [
    "elasticsearch", "query", "builder"
  ],
  "homepage": "https://github.com/pskordilakis/elastin",
  "license": "MIT",
  "authors": [
    {
      "name": "Panagiotis Skordilakis",
      "email": "p.skordilakis@gmail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": "^7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.10.5",
    "phpstan/phpstan-phpunit": "^0.10.0",
    "phpunit/phpunit": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Elastin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "analyze": "phpstan analyse src tests",
    "test": "phpunit"
  }
}
